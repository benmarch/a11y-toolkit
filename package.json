    "globals": "16.3.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.2",
    "typedoc": "0.28.8",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.1",
    "webpack": "5.100.2",
    "webpack-cli": "6.0.1",
    "webpack-node-externals": "3.0.0"
